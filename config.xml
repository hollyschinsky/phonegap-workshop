<widget id="org.devgirl.phonegap.workshop" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>ConferenceApp</name>
    <description>
        Simple conference tracking app.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>

    <!-- Whitelist open -->
    <access origin="*" />

    <!-- Keyboard config -->
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />

    <!-- Status Bar config -->
    <!-- ** Set overlay to false to leave space at the top like iOS6, iOS7 and above made status bar transparent
         Learn all about it here: http://devgirl.org/2014/07/31/phonegap-developers-guid/
     -->

    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#209dc2"/><!-- only applies works when status bar overlay false -->
    <preference name="StatusBarStyle" value="lightcontent" />

    <preference name="DisallowOverscroll" value="true" />

